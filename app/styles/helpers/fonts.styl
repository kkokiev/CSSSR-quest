courier = courier_newregular, sans-serif
ptsans = pt_sansregular, sans-serif
ptsansbold = pt_sansbold, sans-serif

@font-face
	font-family: 'courier_newregular'
	src local('courier_newregular')
	src url('../fonts/cour-webfont.eot')
	src url('../fonts/cour-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/cour-webfont.woff2') format('woff2'),
		 url('../fonts/cour-webfont.woff') format('woff'),
		 url('../fonts/cour-webfont.ttf') format('truetype'),
		 url('../fonts/cour-webfont.svg#courier_newregular') format('svg')
	font-weight normal
	font-style normal

@font-face
	font-family 'pt_sansregular'
	src local('pt_sansregular')
	src url('../fonts/pts55f-webfont.eot')
	src url('../fonts/pts55f-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/pts55f-webfont.woff2') format('woff2'),
		 url('../fonts/pts55f-webfont.woff') format('woff'),
		 url('../fonts/pts55f-webfont.ttf') format('truetype'),
		 url('../fonts/pts55f-webfont.svg#pt_sansregular') format('svg')
	font-weight normal
	font-style normal


@font-face
	font-family: 'pt_sansbold'
	src local('pt_sansbold')
	src url('../fonts/pts75f-webfont.eot')
	src url('../fonts/pts75f-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/pts75f-webfont.woff2') format('woff2'),
		 url('../fonts/pts75f-webfont.woff') format('woff'),
